{
  "name": "bde-isima",
  "appName": "BDE ISIMA",
  "website": "https://bde.isima.fr",
  "version": "4.0.0",
  "scripts": {
    "dev": "blitz start",
    "build": "prisma db push --preview-feature && blitz build",
    "start": "blitz start --production",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": ""
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.1.3",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.0.0",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "11.0.0",
    "@material-ui/core": "^5.0.0-alpha.22",
    "@material-ui/lab": "^5.0.0-alpha.22",
    "@prisma/client": "^2.14.0",
    "blitz": "^0.29.0-canary.0",
    "cuid": "2.1.8",
    "date-fns": "2.16.1",
    "filefy": "0.1.10",
    "final-form": "4.20.1",
    "final-form-arrays": "3.0.2",
    "formidable": "1.2.2",
    "handlebars": "4.7.6",
    "mdi-material-ui": "6.20.0",
    "mui-rff": "2.5.6",
    "next-connect": "0.9.1",
    "next-pwa": "5.0.0",
    "nodemailer": "6.4.17",
    "nprogress": "0.2.0",
    "react": "0.0.0-experimental-4ead6b530",
    "react-dom": "0.0.0-experimental-4ead6b530",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.5.2",
    "react-final-form-arrays": "3.1.3",
    "react-messenger-customer-chat": "0.8.0",
    "react-multi-carousel": "2.5.5",
    "react-swipeable": "6.0.1",
    "react-virtualized-auto-sizer": "1.0.3",
    "react-window": "1.8.6",
    "sanitize-html": "2.2.0",
    "tailwindcss": "2.0.1",
    "victory": "35.4.4",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@prisma/cli": "^2.14.0",
    "@types/faker": "5.1.5",
    "@types/nodemailer": "6.4.0",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "autoprefixer": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.1.0",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "postcss": "8.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "typescript": "4.1.2"
  },
  "private": true
}
