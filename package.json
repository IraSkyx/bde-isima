{
  "name": "bde-isima",
  "appName": "BDE ISIMA",
  "website": "https://bde.isima.fr",
  "version": "4.2.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "seed": "blitz prisma migrate reset && blitz prisma db push && blitz db seed",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.26",
    "@material-ui/lab": "^5.0.0-alpha.26",
    "@prisma/client": "^2.24.1",
    "blitz": "^0.36.4",
    "cuid": "2.1.8",
    "date-fns": "^2.22.1",
    "filefy": "^0.1.11",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "formidable": "1.2.2",
    "handlebars": "^4.7.7",
    "md5": "^2.3.0",
    "mdi-material-ui": "^6.22.1",
    "mjml": "^4.9.3",
    "mjml-react": "^1.2.2",
    "mui-rff": "^3.0.9",
    "next-connect": "^0.10.1",
    "next-pwa": "^5.2.21",
    "nodemailer": "^6.5.0",
    "nprogress": "0.2.0",
    "prisma": "^2.24.1",
    "react": "^0.0.0-experimental-cc4d24ab0",
    "react-dom": "^0.0.0-experimental-cc4d24ab0",
    "react-error-boundary": "^3.1.3",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-messenger-customer-chat": "0.8.0",
    "react-multi-carousel": "^2.6.3",
    "react-palette": "^1.0.2",
    "react-swipeable": "^6.1.2",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-window": "1.8.6",
    "sanitize-html": "^2.4.0",
    "typescript": "~4.1",
    "victory": "^35.8.4",
    "zod": "^1.11.17"
  },
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/nodemailer": "^6.4.2",
    "@types/react": "^17.0.9",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.28.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "prettier-plugin-prisma": "^0.11.0",
    "pretty-quick": "3.1.0",
    "tailwindcss": "^2.1.4"
  }
}
